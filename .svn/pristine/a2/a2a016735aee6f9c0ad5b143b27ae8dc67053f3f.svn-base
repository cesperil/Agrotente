/* #### Font-face Frutiger, Lato, jbecerra #### */

@font-face {
	font-family: 'Frutiger';
	font-style: normal;
	font-weight: normal;
	src: local('Frutiger'), url('../resources/fonts/Frutiger.woff') format('woff');
}

@font-face {
	font-family: 'Lato';
	font-style: normal;
	font-weight: normal;
	src: local('Lato'), url('../resources/fonts/Lato.woff') format('woff');
}

html {
	display: block;
	overflow-x: hidden;
}

body { 
	font-family: Verdana, Arial, Helvetica, sans-serif;
	background-color: #FCFCFC;
