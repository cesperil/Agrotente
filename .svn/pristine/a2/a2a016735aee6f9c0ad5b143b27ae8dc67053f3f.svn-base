	color: #980185 !important;
}

.ui-button.ui-widget.ui-state-default.ui-corner-all.ui-button-text-icon-left.botonera.ui-state-hover {
	background: #EFDAE9 !important;
	border: 1px solid #94328D;
}

.ui-datatable-tablewrapper {
    background-color: #A77FA2 !important;
}

.styleDisabled {
	opacity: .80 !important;
	background-color: #EACEE2 !important;
	color: #6E1F77 !important;
}

/*rsanchez disabled en fechas*/
span.disabledInputCalendar input {
	opacity: .80 !important;
	background-color: #EACEE2 !important;
	color: #6E1F77 !important;
}

/*rsanchez*/
.pe-inputNumber.ui-state-disabled {
	opacity: .80 !important;
	background-color: #EACEE2 !important;
	color: #6E1F77 !important;
}

span.error-validacion input.ui-autocomplete-input {
	border: 1px solid #cd0a0a;
    background: #fef1ec url("/AsistenteAGILE/javax.faces.resource/images/ui-bg_glass_95_fef1ec_1x400.png.xhtml?ln=primefaces-viogen") 50% 50% repeat-x;
    color: #cd0a0a;
}

/*rsanchez estilo para las filas seleccionadas en el datatable*/
.ui-state-highlight a, .ui-widget-content .ui-state-highlight a, .ui-widget-header .ui-state-highlight a {
	color: #5A1756 !important;
}

/* rsanchez fila de titulo de columnas datatable*/
.ui-column-title {
	color: white;
}

.linkColumnas{
	text-decoration: none;
	border: 0px;
	cursor: pointer;
	color: white;
    float: right;
}

span.ui-slider-handle {
	background: white !important; 
}

.botton-230PX input {
	width: 230px;
}


.ui-widget-overlay {
	z-index: 1001 !important;
}


