/** Estilo para el programa de ayudas */
#vertical {
	width: 15em;
	padding: 0;
	margin: 0 auto;
	list-style-type: none;
	font-size: 12pt;
	font-weight: bold;
	font-family: georgia, "times new roman", serif;
}

#vertical li {
	float: left;
	border: 0 em solid #eee;
	margin: 0.1em;
}

#vertical li p {
	text-decoration: none;
	color: #000;
	display: block;
	width: 1.5em;
	height: 1.5em;
	border-top: 0 em solid #000;
	height: auto;
}

#vertical li p em {
	font-style: normal;
	display: block;
	text-align: center;
	border-left: 0 em solid #000;
	border-right: 0 em solid #000;
}

#vertical li p em.nd {
	border-bottom: 0 em solid #000;
}

