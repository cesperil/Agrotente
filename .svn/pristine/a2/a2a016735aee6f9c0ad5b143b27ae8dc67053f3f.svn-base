	margin-left: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	/*overflow: -moz-scrollbars-vertical;*/
	font-size: 12px;
}

/* jbecerra */

span.ui-menuitem-text {
    font-size: 12px;
    padding-left: 12px;
}

/* rsanchez menu activo y compatible para distintos navegadores */
.ui-menuitem-active {
	/* rsanchez_doc Compatibilidad de fondo con navegadores cada uno con su sintaxis */
	background:#94328D -o-linear-gradient(top, rgba(255,255,255,0.6), rgba(255,255,255,0));/* For Opera 11.1 to 12.0 */
    background:#94328D -moz-linear-gradient(top, rgba(255,255,255,0.6), rgba(255,255,255,0)); /* For Firefox 3.6 to 15 */
    background:#94328D linear-gradient(top, rgba(255,255,255,0.6), rgba(255,255,255,0)); /* Standard syntax */ 
	background:#94328D -webkit-gradient(linear, left top, left bottom, from(rgba(255,255,255,0.6)), to(rgba(255,255,255,0))); /*rsanchez_doc Compatibilidad de fondo con chrome*/
	
	box-shadow:0px 0px 8px #94328D;
	color: #FFFFFF !important;
}

.ui-menuitem-link:hover span {
	color: #FFFFFF !important;
}

/* Tab panel jbecerra */
div.ui-tabs {
	padding: 0;
	border-top: 0;
	border-right: 0;
	top: -1px;
	margin-right: 15px;
}

/* rsanchez pesta�a seleccionada */
.ui-tabs-selected {
	background:#94328D -o-linear-gradient(top, rgba(255,255,255,0.6), rgba(255,255,255,0)) !important;/* For Opera 11.1 to 12.0 */
    background:#94328D -moz-linear-gradient(top, rgba(255,255,255,0.6), rgba(255,255,255,0)) !important; /* For Firefox 3.6 to 15 */
    background:#94328D linear-gradient(top, rgba(255,255,255,0.6), rgba(255,255,255,0)) !important; /* Standard syntax */ 
	background:#94328D -webkit-gradient(linear, left top, left bottom, from(rgba(255,255,255,0.6)), to(rgba(255,255,255,0))) !important; /*rsanchez_doc Compatibilidad de fondo con chrome*/
	box-shadow:0px 0px 8px #94328D !important;
	color: #FFFFF !important;
}

ul.ui-tabs-nav.ui-helper-reset.ui-helper-clearfix.ui-widget-header.ui-corner-all {
	border-bottom: 1px solid #cba1c7;
}

li.ui-state-default {
	border-bottom: 1px solid #cba1c7;
}

li.ui-state-active {
	top: 2px !important;
}

div.ui-tabs-panels {
	margin-top: -1px;
	border-right: 1px solid #cba1c7;
	}

.ui-tabs.ui-tabs-top > .ui-tabs-nav li {
	left: -1px;
}

.ui-state-active, .ui-widget-content .ui-state-active, .ui-widget-header .ui-state-active {
    border: 1px solid #cba1c7;
}

.ui-tabs.ui-tabs-top > .ui-tabs-nav {
	padding: 0;
}

.ui-tabs-nav {
	background: none;
}

#contenidoCentral .ui-datatable-header.ui-widget-header.ui-corner-top {
    text-align: left;
    font-size: 11px;
}

.ui-datatable-footer.ui-widget-header.ui-corner-bottom {
    text-align: left;
}


